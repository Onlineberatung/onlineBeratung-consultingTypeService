        "value": {
          "type": "boolean"
        },
        "readOnly": {
          "type": "boolean"
        }
      },
      "required": [
        "value",
        "readOnly"
      ]
    },
    "multitenancyEnabled": {
      "type": "object",
      "properties": {
        "value": {
          "type": "boolean"
        },
        "readOnly": {
          "type": "boolean"
        }
      },
      "required": [
        "value",
        "readOnly"
      ]
    },
    "useTenantService": {
      "type": "object",
      "properties": {
        "value": {
          "type": "boolean"
        },
        "readOnly": {
          "type": "boolean"
        }
      },
      "required": [
        "value",
        "readOnly"
      ]
    },
    "enableTenantTheming": {
      "type": "object",
      "properties": {
        "value": {
          "type": "boolean"
        },
